<?php

namespace XTeam\PlatformBundle\Entity;

use FOS\UserBundle\Entity\User as BaseUser;
use Doctrine\ORM\Mapping as ORM;

/**
 * fos_user
 */
class User extends BaseUser
{
    /**
     * @var int
     */
    protected $id;

    /**
     * @var string
     */
    private $nom;

    /**
     * @var string
     */
    private $prenom;

    /**
     * @var string
     */
    private $profession;

    /**
     * @var string
     */
    private $business;

    /**
     * @var string
     */
    private $statut;

    /**
     * @var string
     */
    private $promo;

    /**
     * @var string
     */
    private $git;

    /**
     * @var string
     */
    private $twitter;

    /**
     * @var string
     */
    private $facebook;

    /**
     * @var string
     */
    private $linkedin;

    /**
     * @var string
     */
    private $avatar;


    public function __construct()
    {
        parent::__construct();
    }

}